<div class="container navigation">
  <div class="row">
    <div class="col-12 pt-3 pb-1">
      <div class="float-end">
        <% if @current_user.present? %>
        <div class="dropdown show">
          <a class="btn btn-white dropdown-toggle" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            <%= @current_user.name %>
          </a>
          <div class="container">
            <nav>
              <%= link_to 'Home', root_path %>
              <% if @current_user.present? %>


              <%= link_to "Log out #{ @current_user.email }", login_path, :method => 'delete' %>
              <% else %>
              <%= link_to 'Sign up', new_user_path %>
              <%= link_to 'Sign in', login_path %>
              <% end %>
            </nav>
        
          </div>




        </div>

        <% else %>
        <%= link_to "Log In", login_path, class: "btn btn-outline-primary" %>
        <%= link_to "Sign Up", new_user_path, class: "btn btn-primary" %>
        <% end %>
      </div>
      <img class="home-logo" src="https://scontent-syd2-1.xx.fbcdn.net/v/t1.18169-9/14355038_117843045342969_2416252482737794925_n.jpg?_nc_cat=109&ccb=1-5&_nc_sid=09cbfe&_nc_ohc=iVxj7CcIPlQAX9ge8u-&_nc_ht=scontent-syd2-1.xx&oh=23fa9a3c64d7b607979668d12589eb03&oe=61A009BE" alt="">
      <p class="h5 mb-4 text-center"><%= link_to "Home", root_path %> | <%= link_to "Chat", chats_path %> | <%= link_to "Subjects", subjects_path %> | <%= link_to "Tutors", tutors_path %></p>
    </div>

  </div>
</div>
